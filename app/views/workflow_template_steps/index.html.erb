<h1>Listing workflow_template_steps</h1>

<table>
  <thead>
    <tr>
      <th>Workflow template step</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_template_steps.each do |workflow_template_step| %>
      <tr>
        <td><%= workflow_template_step.workflow_template_step %></td>
        <td><%= link_to 'Show', workflow_template_step %></td>
        <td><%= link_to 'Edit', edit_workflow_template_step_path(workflow_template_step) %></td>
        <td><%= link_to 'Destroy', workflow_template_step, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow template step', new_workflow_template_step_path %>

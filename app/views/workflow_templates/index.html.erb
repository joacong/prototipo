<h1>Listing workflow_templates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Workflow template</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_templates.each do |workflow_template| %>
      <tr>
        <td><%= workflow_template.name %></td>
        <td><%= workflow_template.workflow_template %></td>
        <td><%= link_to 'Show', workflow_template %></td>
        <td><%= link_to 'Edit', edit_workflow_template_path(workflow_template) %></td>
        <td><%= link_to 'Destroy', workflow_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow template', new_workflow_template_path %>

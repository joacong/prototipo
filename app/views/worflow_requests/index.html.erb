<h1>Listing worflow_requests</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @worflow_requests.each do |worflow_request| %>
      <tr>
        <td><%= worflow_request.user %></td>
        <td><%= link_to 'Show', worflow_request %></td>
        <td><%= link_to 'Edit', edit_worflow_request_path(worflow_request) %></td>
        <td><%= link_to 'Destroy', worflow_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Worflow request', new_worflow_request_path %>
